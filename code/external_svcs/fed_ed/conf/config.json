{
    "app": "fed_ed",
    "port": 3000,
    "environment": "development",
    "privateKey": "private.key",
    "connectionString": "mysql://root:secret@localhost:3306/fed_ed",
    "errorFile": "logs/error.log",
    "combineFile": "logs/combined.log",
    "cors": {
      "development": {
        "corsOptions": {
          "origin": "*",
          "methods": "GET,PUT,POST,DELETE"
        }
      },
      "production": {
        "corsOptions": {
          "origin": [
            "https://example.com",
            "http://example.com"
          ],
          "methods": ["GET", "PUT", "POST", "DELETE"]
        }
      }
    }
  }